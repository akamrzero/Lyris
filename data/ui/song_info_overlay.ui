<interface>
  <!-- interface-name song_info_overlay.ui -->
  <requires lib="gtk" version="4.18"/>
  <requires lib="libadwaita" version="1.7"/>
  <template class="SongInfoOverlay" parent="AdwBin">
    <child>
      <object class="AdwBottomSheet" id="edit_bottom_sheet_overlay">
        <property name="content">
          <object class="AdwBottomSheet" id="info_bottom_sheet_overlay">
            <property name="content">
              <object class="AdwBin" id="main_content"/>
            </property>
            <property name="sheet">
              <object class="AdwBin" id="info_sheet">
                <property name="child">
                  <object class="GtkBox">
                    <property name="css-classes">lyris-small-padding-container</property>
                    <property name="orientation">vertical</property>
                    <property name="spacing">12</property>
                    <child>
                      <object class="GtkBox">
                        <property name="margin-top">36</property>
                        <property name="spacing">12</property>
                        <child>
                          <object class="GtkBox">
                            <property name="css-classes">lyris-large-rounded-corners</property>
                            <property name="overflow">hidden</property>
                            <child>
                              <object class="AdwBin" id="album_art_img_container"/>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkLabel" id="title_label">
                            <property name="css-classes">title-1</property>
                            <property name="ellipsize">end</property>
                            <property name="hexpand">True</property>
                            <property name="xalign">0.0</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox">
                        <property name="height-request">64</property>
                        <property name="spacing">6</property>
                        <child>
                          <object class="GtkButton" id="queue_button">
                            <property name="child">
                              <object class="AdwButtonContent"/>
                            </property>
                            <property name="css-classes">pill</property>
                            <property name="hexpand">True</property>
                            <property name="icon-name">list-</property>
                            <property name="label">Add to Queue</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkButton" id="share_btn">
                            <property name="css-classes">pill</property>
                            <property name="halign">end</property>
                            <property name="icon-name">preferences-system-sharing</property>
                            <property name="width-request">64</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkButton" id="edit_btn">
                            <property name="css-classes">pill</property>
                            <property name="halign">end</property>
                            <property name="height-request">60</property>
                            <property name="icon-name">document-edit-symbolic</property>
                            <property name="valign">center</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox">
                        <property name="height-request">64</property>
                        <child>
                          <object class="GtkButton" id="play_btn">
                            <property name="child">
                              <object class="AdwButtonContent">
                                <property name="icon-name">media-playback-start-symbolic</property>
                                <property name="label">Play</property>
                              </object>
                            </property>
                            <property name="css-classes">suggested-action
pill</property>
                            <property name="hexpand">True</property>
                            <property name="hexpand-set">True</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkListBox">
                        <property name="css-classes">boxed-list
lyris-listbox-item-padding</property>
                        <property name="selection-mode">none</property>
                        <child>
                          <object class="AdwActionRow" id="duration_label">
                            <property name="icon-name">clock-alt-symbolic</property>
                            <property name="title">Duration</property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow" id="album_label">
                            <property name="icon-name">library-music-symbolic</property>
                            <property name="title">Album</property>
                          </object>
                        </child>
                        <child>
                          <object class="AdwActionRow" id="artist_label">
                            <property name="icon-name">music-artist-symbolic</property>
                            <property name="title">Artist</property>
                          </object>
                        </child>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox">
                        <child>
                          <object class="GtkButton" id="close_info_button">
                            <property name="css-classes">pill</property>
                            <property name="halign">end</property>
                            <property name="hexpand">True</property>
                            <property name="label">Close</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </property>
              </object>
            </property>
          </object>
        </property>
        <property name="sheet">
          <object class="AdwBin" id="edit_sheet">
            <property name="child">
              <object class="GtkBox">
                <property name="css-classes">lyris-small-padding-container</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkBox">
                    <property name="margin-top">36</property>
                    <child>
                      <object class="GtkLabel">
                        <property name="css-classes">title-1</property>
                        <property name="hexpand">True</property>
                        <property name="hexpand-set">True</property>
                        <property name="label">Edit Song</property>
                        <property name="xalign">0.0</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkButton">
                        <property name="css-classes">circular</property>
                        <property name="halign">end</property>
                        <property name="icon-name">dialog-question-symbolic</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkListBox">
                    <property name="css-classes">boxed-list</property>
                    <property name="margin-bottom">24</property>
                    <property name="margin-top">24</property>
                    <property name="selection-mode">none</property>
                    <child>
                      <object class="AdwEntryRow" id="title_input">
                        <property name="title">Title</property>
                      </object>
                    </child>
                    <child>
                      <object class="AdwEntryRow" id="artist_input">
                        <property name="title">Artist</property>
                      </object>
                    </child>
                    <child>
                      <object class="AdwEntryRow" id="genre_input">
                        <property name="title">Genre</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <child>
                          <object class="GtkLabel">
                            <property name="label">Title</property>
                            <property name="xalign">0.0</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkTextView">
                            <property name="buffer">
                              <object class="GtkTextBuffer"/>
                            </property>
                            <property name="height-request">200</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkBox">
                    <property name="halign">end</property>
                    <property name="spacing">12</property>
                    <child>
                      <object class="GtkButton" id="cancel_btn">
                        <property name="css-classes">pill</property>
                        <property name="label">Cancel</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkButton">
                        <property name="css-classes">pill
suggested-action</property>
                        <property name="label">Save</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </property>
          </object>
        </property>
      </object>
    </child>
  </template>
</interface>
